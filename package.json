{
  "name": "sei-portfolio-ai",
  "version": "1.0.0",
  "description": "AI-powered conversational portfolio manager for Sei Network with integrated lending and liquidity management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "api": "tsx src/api/server.ts",
    "api:dev": "tsx watch src/api/server.ts",
    "api:build": "tsc --project tsconfig.json",
    "api:prod": "node dist/api/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:e2e": "jest test/e2e",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:integration:docker": "jest test/integration --ci --runInBand --forceExit --detectOpenHandles",
    "test:e2e:docker": "jest test/e2e --ci --runInBand --testTimeout=60000 --forceExit",
    "test:property:docker": "jest test/integration/property --ci --maxWorkers=2",
    "test:performance:docker": "jest test/integration/performance --ci --runInBand --testTimeout=300000",
    "test:docker:all": "jest --ci --coverage --maxWorkers=4 --verbose --forceExit",
    "test:docker:quick": "jest test/integration/protocols --ci --runInBand --bail",
    "docker:test:up": "docker-compose -f docker-compose.test.yml up -d",
    "docker:test:down": "docker-compose -f docker-compose.test.yml down -v",
    "docker:test:logs": "docker-compose -f docker-compose.test.yml logs -f test-runner",
    "docker:test:run": "docker-compose -f docker-compose.test.yml up --abort-on-container-exit",
    "docker:test:clean": "docker-compose -f docker-compose.test.yml down -v --rmi all --remove-orphans",
    "e2e:up": "docker-compose -f docker-compose.e2e.yml up -d --build",
    "e2e:down": "docker-compose -f docker-compose.e2e.yml down -v",
    "e2e:run": "docker-compose -f docker-compose.e2e.yml run --rm cypress",
    "e2e:open": "cypress open",
    "e2e:local": "cypress run",
    "e2e:test": "npm run e2e:up && npm run e2e:run; npm run e2e:down",
    "e2e:clean": "docker-compose -f docker-compose.e2e.yml down -v --rmi all --remove-orphans",
    "e2e:logs": "docker-compose -f docker-compose.e2e.yml logs -f",
    "test:performance": "jest test/performance --testTimeout=300000",
    "test:performance:sei": "jest test/performance/sei --testTimeout=300000",
    "test:performance:protocols": "jest test/performance/protocols --testTimeout=300000",
    "test:performance:memory": "jest test/performance/memory --testTimeout=300000",
    "test:performance:load": "docker-compose -f docker-compose.perf.yml run --rm load-generator",
    "test:performance:stress": "docker-compose -f docker-compose.perf.yml run --rm load-generator stress",
    "test:performance:endurance": "docker-compose -f docker-compose.perf.yml run --rm load-generator endurance",
    "test:performance:spike": "docker-compose -f docker-compose.perf.yml run --rm load-generator spike",
    "test:performance:protocol": "docker-compose -f docker-compose.perf.yml run --rm load-generator protocol",
    "test:performance:benchmark": "docker-compose -f docker-compose.perf.yml run --rm load-generator benchmark",
    "performance:start": "docker-compose -f docker-compose.perf.yml up -d",
    "performance:stop": "docker-compose -f docker-compose.perf.yml down",
    "performance:logs": "docker-compose -f docker-compose.perf.yml logs -f",
    "performance:monitor": "open http://localhost:3000",
    "performance:report": "node test/performance/scripts/generate-report.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "train": "ts-node scripts/train-model.ts",
    "evaluate": "ts-node scripts/evaluate-model.ts",
    "deploy": "ts-node scripts/deploy-model.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "collect": "tsx scripts/collect.ts",
    "collect:start": "tsx scripts/collect.ts start",
    "collect:interactive": "tsx scripts/collect.ts interactive",
    "collect:status": "tsx scripts/collect.ts status",
    "process": "tsx scripts/process.ts",
    "process:run": "tsx scripts/process.ts run",
    "process:validate": "tsx scripts/process.ts validate",
    "export": "tsx scripts/export.ts",
    "export:dataset": "tsx scripts/export.ts dataset",
    "export:cloud": "tsx scripts/export.ts cloud"
  },
  "keywords": [
    "sei-network",
    "defi",
    "portfolio-management",
    "ai-chat",
    "lending",
    "liquidity-management",
    "yei-finance",
    "dragonswap",
    "conversational-ai",
    "yield-optimization"
  ],
  "author": "Sei Portfolio AI Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.450.0",
    "@google-cloud/storage": "^7.5.0",
    "@json2csv/plainjs": "^7.0.0",
    "@langchain/community": "^0.3.48",
    "@langchain/core": "^0.3.62",
    "@sei-js/cosmjs": "^1.0.11",
    "@sei-js/evm": "^2.0.5",
    "@sei-js/proto": "^4.1.0",
    "ai": "^4.2.49",
    "archiver": "^6.0.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.0",
    "ethers": "^6.8.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "express-slow-down": "^2.0.3",
    "fp-ts": "^2.16.1",
    "glob": "^10.3.10",
    "helmet": "^8.0.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "openai": "^4.74.0",
    "ora": "^7.0.1",
    "p-limit": "^5.0.0",
    "rate-limiter-flexible": "^5.0.3",
    "typescript": "^5.2.0",
    "uuid": "^9.0.1",
    "viem": "^2.0.0",
    "ws": "^8.14.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.0",
    "@fast-check/jest": "^1.1.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.0",
    "@types/uuid": "^9.0.6",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "cypress": "^13.6.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fast-check": "^3.13.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "msw": "^2.0.0",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.6.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sei-network/portfolio-ai.git"
  },
  "bugs": {
    "url": "https://github.com/sei-network/portfolio-ai/issues"
  },
  "homepage": "https://github.com/sei-network/portfolio-ai#readme",
  "directories": {
    "lib": "src",
    "test": "test",
    "doc": "docs"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "LICENSE"
  ]
}
