# Server Configuration
PORT=8000
NODE_ENV=development
LOG_LEVEL=info

# Frontend URL
FRONTEND_URL=http://localhost:3000

# Database
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Redis
REDIS_URL=redis://localhost:6379

# OpenAI - Required for AI functionality
OPENAI_API_KEY=your_openai_api_key

# Sei Network Configuration
SEI_NETWORK=testnet
SEI_CHAIN_ID=atlantic-2
SEI_RPC_URL=https://rpc-testnet.sei.io
SEI_EVM_RPC_URL=https://evm-rpc-testnet.sei.io

# Hive Intelligence Adapter
HIVE_ENABLED=true
# Uses OPENAI_API_KEY configured above
HIVE_BASE_URL=https://api.dexscreener.com/latest
HIVE_RATE_LIMIT=60
HIVE_CACHE_ENABLED=true
HIVE_CACHE_TTL=300000

# Sei Agent Kit Adapter
SAK_ENABLED=true
# Optional: Private key for transaction signing
SAK_WALLET_PRIVATE_KEY=
# Optional: Mnemonic for wallet generation
SAK_WALLET_MNEMONIC=
SAK_RATE_LIMIT=60

# Sei MCP Adapter
MCP_ENABLED=true
MCP_ENDPOINT=localhost
MCP_PORT=8765
MCP_SECURE=false
MCP_API_KEY=
MCP_CONNECTION_TIMEOUT=30000
MCP_HEARTBEAT_INTERVAL=30000
MCP_MAX_RECONNECT_ATTEMPTS=5

# Protocol Configurations
TAKARA_ENABLED=true
SYMPHONY_ENABLED=true
DRAGONSWAP_ENABLED=true
SILO_ENABLED=true