# Privy Configuration
VITE_PRIVY_APP_ID=your_privy_app_id_here
VITE_PRIVY_CLIENT_ID=

# WalletConnect Project ID (optional, for better wallet support)
# Get your project ID from https://cloud.walletconnect.com/
VITE_WALLETCONNECT_PROJECT_ID=

# Sei Network RPC (optional, defaults to public RPC)
VITE_SEI_RPC_URL=https://evm-rpc.sei-apis.com

# API Configuration
# For local development with separate backend: http://localhost:3001
# For Vercel deployment: leave empty (uses relative paths)
VITE_API_URL=
VITE_WS_URL=

# Orchestrator API Configuration
# For local development: http://localhost:3001 and ws://localhost:3001
# For Vercel deployment: leave empty
VITE_ORCHESTRATOR_API=
VITE_ORCHESTRATOR_WS=

# Optional: Override the internal orchestrator endpoints
# For local development: http://localhost:3001 and ws://localhost:3001
# For Vercel deployment: leave empty
ORCHESTRATOR_ENDPOINT=
ORCHESTRATOR_WS_ENDPOINT=

# ElevenLabs Configuration (for voice features)
# ⚠️ IMPORTANT: The API key is now server-side only for security
# Set ELEVENLABS_API_KEY in Vercel Dashboard > Project Settings > Environment Variables
# Only the voice ID is needed client-side:
VITE_ELEVENLABS_VOICE_ID=your_voice_id_here
VITE_VOICE_ENABLED=true

# Voice Settings (optional)
VITE_VOICE_STABILITY=0.75
VITE_VOICE_SIMILARITY_BOOST=0.75
VITE_VOICE_STYLE=0.5
VITE_VOICE_USE_SPEAKER_BOOST=true

# AI Integration Configuration
# ⚠️ IMPORTANT: These are server-side variables for Vercel Functions
# Set these in Vercel Dashboard > Project Settings > Environment Variables
# DO NOT commit actual API keys to source control!

# OpenAI Configuration (REQUIRED for AI features)
# Server-side only - Set in Vercel environment variables
# OPENAI_API_KEY=sk-your_openai_api_key_here

# Anthropic Configuration (Optional - for Claude integration)
# Server-side only - Set in Vercel environment variables  
# ANTHROPIC_API_KEY=sk-ant-your_anthropic_api_key_here

# Supabase Configuration (REQUIRED for data persistence)
# Public client-side variables
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here