{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:clean": "rm -rf node_modules/.vite && vite",
    "dev:debug": "NODE_ENV=development vite --mode development",
    "build": "NODE_ENV=production vite build --mode production",
    "build:debug": "tsc --noEmit --noUnusedLocals false --noUnusedParameters false && NODE_ENV=development vite build --mode development",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config=jest.integration.config.js",
    "test:integration:watch": "jest --config=jest.integration.config.js --watch",
    "test:integration:coverage": "jest --config=jest.integration.config.js --coverage",
    "test:performance": "jest --config=jest.performance.config.js",
    "test:accessibility": "jest --config=jest.accessibility.config.js",
    "test:visual": "jest --config=jest.visual.config.js",
    "test:bundle": "node scripts/test-bundle-optimization.js",
    "test:lazy": "node scripts/test-lazy-loading.js",
    "test:all": "npm run test && npm run test:integration && npm run test:performance && npm run test:accessibility && npm run test:bundle",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:integration:ci": "jest --config=jest.integration.config.js --ci --coverage --watchAll=false",
    "analyze:bundle": "npm run build && npx vite-bundle-analyzer dist",
    "preload:features": "node scripts/preload-features.js",
    "setup:walletconnect": "node scripts/setup-walletconnect.js",
    "test:walletconnect": "node scripts/test-walletconnect-config.js",
    "test:diagnostics": "node scripts/test-diagnostics.js",
    "test:voice": "./scripts/run-voice-tests.sh",
    "test:voice:basic": "RUN_BASIC=true RUN_PERMISSIONS=false RUN_VAD=false RUN_ERROR_RECOVERY=false RUN_MEMORY=false RUN_MOBILE=false ./scripts/run-voice-tests.sh",
    "test:voice:permissions": "RUN_BASIC=false RUN_PERMISSIONS=true ./scripts/run-voice-tests.sh",
    "test:voice:vad": "RUN_BASIC=false RUN_VAD=true ./scripts/run-voice-tests.sh",
    "test:voice:mobile": "RUN_BASIC=false RUN_MOBILE=true ./scripts/run-voice-tests.sh",
    "test:voice:cross-browser": "RUN_CROSS_BROWSER=true ./scripts/run-voice-tests.sh",
    "test:voice:performance": "RUN_PERFORMANCE=true ./scripts/run-voice-tests.sh",
    "test:voice:docker": "docker-compose -f docker/docker-compose.voice-tests.yml up --build",
    "test:voice:playwright": "npx playwright test e2e/voice-chat/ --project=voice-chat-chrome",
    "test:voice:playwright:mobile": "npx playwright test e2e/voice-chat/voice-mobile.spec.ts --project=voice-mobile-iPhone",
    "test:api": "node scripts/test-api-endpoints.js",
    "test:api:report": "node scripts/test-api-endpoints.js --report",
    "test:production": "node scripts/test-production-deployment.js",
    "test:production:report": "node scripts/test-production-deployment.js --report --verbose",
    "test:quick": "node scripts/run-all-tests.js quick",
    "test:full": "node scripts/run-all-tests.js full",
    "test:report": "npm run test:full && npm run test:production:report",
    "validate": "./scripts/quick-validation.sh",
    "test:validate": "./scripts/quick-validation.sh"
  },
  "overrides": {
    "ox": "0.8.1",
    "postcss": "^8.4.49"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@anthropic-ai/sdk": "^0.32.1",
    "@elevenlabs/client": "^0.3.0",
    "@privy-io/react-auth": "^2.17.3",
    "@privy-io/wagmi": "^1.0.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.50.3",
    "@tanstack/react-query": "^5.81.5",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/kv": "^2.0.0",
    "ai": "^4.3.16",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.0.8",
    "fp-ts": "^2.16.10",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.525.0",
    "next-auth": "^4.24.11",
    "openai": "^5.8.2",
    "postcss": "^8.4.49",
    "react": "^18",
    "react-dom": "^18",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^6.30.1",
    "rxjs": "^7.8.2",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.1",
    "viem": "^2.31.6",
    "wagmi": "^2.15.6",
    "zod": "^3.24.1"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@axe-core/puppeteer": "^4.8.5",
    "@jest/environment": "^29.7.0",
    "@playwright/test": "^1.53.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vercel/node": "^5.3.4",
    "@vitejs/plugin-react": "^4.4.4",
    "axe-core": "^4.8.4",
    "eslint": "^8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "fast-check": "^4.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.6.0",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "puppeteer": "^22.15.0",
    "terser": "^5.32.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5",
    "vite": "^6.1.5",
    "vite-plugin-node-polyfills": "^0.24.0",
    "web-vitals": "^5.0.3"
  }
}
